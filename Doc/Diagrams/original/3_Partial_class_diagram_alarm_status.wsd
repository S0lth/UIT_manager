@startuml
title partial class diagram alarm status
Class Alarm{
- date : DateTime
}

class AlarmHistory{
-modificationDate : DateTime
}

class AlarmStatusType{
- name : String
- description : string
}

class Employee{
- lastName : String
- firstName : String 
- role : String
}

Alarm "1" --right-- "1-N" AlarmHistory: "has >"
AlarmStatusType "1" <--left-- AlarmHistory: "< has"
AlarmHistory --> "0..1" Employee : "is modified by >"
@enduml